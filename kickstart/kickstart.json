{
  "variables": {
    "defaultTenantId": "d7d09513-a3f5-401c-9685-34ab6c552453",
    "adminEmail": "admin@example.com",
    "adminPassword": "password",
    "adminUserId": "00000000-0000-0000-0000-000000000001",
    "applicationName": "MCP Tools Example",
    "applicationId": "e9fdb985-9173-4e01-9d73-ac2d60d1dc8e",
    "adminApiKey": "bf69486b-4733-4470-a592-f1bfce7admin",
    "apiKey": "bf69486b-4733-4470-a592-f1bfce7af580",
    "asymmetricKeyId": "#{UUID()}"
  },
  "apiKeys": [
    {
      "key": "#{adminApiKey}",
      "description": "Unrestricted API key"
    },
    {
      "key": "#{apiKey}",
      "description": "MCP Tools API Key",
      "permissions": {
        "endpoints": {
          "/api/application": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/application/*": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/user": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/user/*": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/user/search": [
            "POST"
          ],
          "/api/group": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/group/*": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/tenant": [
            "POST",
            "GET",
            "PUT",
            "PATCH",
            "DELETE"
          ]
        }
      }
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/key/generate/#{asymmetricKeyId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "key": {
          "algorithm": "RS256",
          "name": "For example apps",
          "length": 2048
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user",
      "body": {
        "user": {
          "email": "#{adminEmail}",
          "password": "#{adminPassword}",
          "firstName": "Admin",
          "lastName": "User",
          "data": {
            "role": "admin"
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{adminUserId}",
      "body": {
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": [
            "admin"
          ]
        },
        "roles": [
          "admin"
        ],
        "skipRegistrationVerification": true,
        "user": {
          "birthDate": "1981-06-04",
          "email": "#{adminEmail}",
          "firstName": "Admin",
          "lastName": "UI",
          "password": "#{adminPassword}"
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{applicationId}",
      "body": {
        "application": {
          "name": "#{applicationName}",
          "roles": [
            {
              "name": "admin",
              "description": "Administrator role",
              "isDefault": false,
              "isSuperRole": false
            },
            {
              "name": "user",
              "description": "Standard user role",
              "isDefault": true,
              "isSuperRole": false
            }
          ],
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:3001/api/auth/callback/fusionauth",
              "http://localhost:3002/auth/callback"
            ],
            "clientSecret": "super-secret-secret-that-should-be-regenerated-for-production",
            "enabledGrants": [
              "authorization_code",
              "refresh_token"
            ],
            "generateRefreshTokens": true,
            "logoutURL": "http://localhost:3001",
            "requireClientAuthentication": true
          },
          "jwtConfiguration": {
            "enabled": true,
            "accessTokenKeyId": "#{asymmetricKeyId}",
            "idTokenKeyId": "#{asymmetricKeyId}"
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user",
      "body": {
        "user": {
          "email": "user@example.com",
          "password": "password",
          "firstName": "Test",
          "lastName": "User",
          "username": "testuser"
        },
        "registration": {
          "applicationId": "#{applicationId}",
          "roles": [
            "user"
          ]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user",
      "body": {
        "user": {
          "email": "jane.doe@example.com",
          "password": "password",
          "firstName": "Jane",
          "lastName": "Doe",
          "username": "janedoe"
        },
        "registration": {
          "applicationId": "#{applicationId}",
          "roles": [
            "user"
          ]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user",
      "body": {
        "user": {
          "email": "john.smith@example.com",
          "password": "password",
          "firstName": "John",
          "lastName": "Smith",
          "username": "johnsmith"
        },
        "registration": {
          "applicationId": "#{applicationId}",
          "roles": [
            "admin"
          ]
        }
      }
    }
  ]
}